  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="SafeFiles\Objects\Background\bg1.txt">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="SafeFiles\Sound\SoundSetup.txt">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
