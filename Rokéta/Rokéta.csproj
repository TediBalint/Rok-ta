    <None Update="SafeFiles\Objects\Enemy2.txt">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="SafeFiles\Objects\Enemy3.txt">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="SafeFiles\Objects\Players\Player2.txt">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="SafeFiles\Objects\R.txt">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="SafeFiles\Default\" />
    <Folder Include="SafeFiles\Objects\Players\" />
  </ItemGroup>

</Project>
